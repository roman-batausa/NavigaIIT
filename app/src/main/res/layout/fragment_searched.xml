<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SearchedFragment"
    android:id="@+id/searched_fragment"
    android:background="@color/white_na_grey_gamay">

    <com.ortiz.touchview.TouchImageView
        android:id="@+id/searchedImage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/first_floor_no_path"
        android:layout_marginTop="-200dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/bding_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="building"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        android:shadowColor="@color/white_na_grey_gamay"
        android:shadowRadius="5"
        android:layout_marginTop="80dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/qwer"
        android:shadowColor="@color/white_na_grey_gamay"
        android:shadowRadius="5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/off_white"
        android:layout_weight="1"
        android:inputType="none"
        android:layout_marginBottom="5dp"
        android:text="Room"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="20sp"
        android:layout_marginStart="10dp"
        android:textColor="@color/maroon"
        android:paddingTop="5dp"
        android:layout_marginTop="401dp"
        android:layout_marginEnd="190dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/searched_textView_room"
        android:shadowColor="@color/white_na_grey_gamay"
        android:shadowRadius="5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/off_white"
        android:layout_weight="1"
        android:inputType="none"
        android:layout_marginBottom="5dp"
        android:text="Select Room"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_marginStart="10dp"
        android:textColor="@color/black"
        android:paddingTop="5dp"
        android:layout_marginEnd="190dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/qwer"/>

    <TextView
        android:id="@+id/asdf"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="401dp"
        android:layout_marginBottom="5dp"
        android:layout_weight="1"
        android:backgroundTint="@color/off_white"
        android:inputType="none"
        android:paddingTop="5dp"
        android:shadowColor="@color/white_na_grey_gamay"
        android:shadowRadius="5"
        android:text="Floor"
        android:textAlignment="center"
        android:textColor="@color/maroon"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginStart="190dp"
        android:layout_marginEnd="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/searched_textView_floor"
        android:shadowColor="@color/white_na_grey_gamay"
        android:shadowRadius="5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/off_white"
        android:layout_weight="1"
        android:inputType="none"
        android:layout_marginBottom="5dp"
        android:text="1st"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="20dp"
        android:paddingTop="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginStart="190dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/asdf"/>

    <TextView
        android:id="@+id/desc_top2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Room descriptions: "
        android:layout_marginTop="410dp"
        android:padding="10dp"
        android:textSize="16sp"
        android:lineSpacingExtra="5dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:shadowColor="@color/white_na_grey_gamay"
        android:shadowRadius="5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
        android:id="@+id/room_descriptions2_textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Room descriptions: "
        android:layout_marginTop="440dp"
        android:padding="10dp"
        android:textSize="14sp"
        android:lineSpacingExtra="5dp"
        android:textColor="@color/black"
        android:shadowColor="@color/white_na_grey_gamay"
        android:shadowRadius="5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/note_top2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=""
        android:layout_marginTop="110dp"
        android:padding="10dp"
        android:textSize="16sp"
        android:lineSpacingExtra="5dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:shadowColor="@color/white_na_grey_gamay"
        android:shadowRadius="5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
        android:id="@+id/note2_textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=""
        android:layout_marginTop="140dp"
        android:padding="10dp"
        android:textSize="14sp"
        android:lineSpacingExtra="5dp"
        android:textColor="@color/black"
        android:shadowColor="@color/white_na_grey_gamay"
        android:shadowRadius="5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_bt3"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/iit_yellow"
        android:src="@drawable/baseline_bookmark_add_40"
        app:fabSize="normal"
        android:layout_marginBottom="67dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>